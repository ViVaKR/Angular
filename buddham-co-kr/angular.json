{
  "$schema": "node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "buddham-co-kr": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:class": {
          "skipTests": true
        },
        "@schematics/angular:component": {
          "skipTests": true
        },
        "@schematics/angular:config": {
          "skipTests": true,
          "path": "src/configs"
        },
        "@schematics/angular:lib": {
          "path": "src/libraries"
        },
        "@schematics/angular:enum": {
          "path": "src/enums"
        },
        "@schematics/angular:directive": {
          "skipTests": true,
          "path": "directives"
        },
        "@schematics/angular:guard": {
          "skipTests": true,
          "path": "src/guards"
        },
        "@schematics/angular:interceptor": {
          "skipTests": true,
          "path": "src/interceptors"
        },
        "@schematics/angular:interface": {
          "path": "src/interfaces"
        },
        "@schematics/angular:pipe": {
          "skipTests": true,
          "path": "src/pipes"
        },
        "@schematics/angular:resolver": {
          "skipTests": true,
          "path": "src/resolvers"
        },
        "@schematics/angular:service": {
          "skipTests": true,
          "path": "src/services"
        },
        "@schematics/angular:module": {
          "skipTests": true,
          "path": "src/modules"
        },
        "@schematics/angular:app-shell": {
          "skipTests": true,
          "path": "src/app-shells"
        },
        "@schematics/angular:service-worker": {
          "skipTests": true,
          "path": "src/service-workers"
        },
        "@schematics/angular:web-worker": {
          "path": "src/web-workers"
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": "dist/buddham-co-kr",
            "index": "src/index.html",
            "browser": "src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "tsconfig.app.json",
            "assets": [
              {
                "glob": "**/*",
                "input": "public"
              }
            ],
            "styles": [
              "@angular/material/prebuilt-themes/azure-blue.css",
              "src/styles.css"
            ],
            "scripts": [],
            "server": "src/main.server.ts",
            "prerender": true,
            "ssr": {
              "entry": "src/server.ts"
            }
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2048kB",
                  "maximumError": "5MB"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "4kB",
                  "maximumError": "8kB"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "buddham-co-kr:build:production"
            },
            "development": {
              "buildTarget": "buddham-co-kr:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "tsconfig.spec.json",
            "assets": [
              {
                "glob": "**/*",
                "input": "public"
              }
            ],
            "styles": [
              "@angular/material/prebuilt-themes/azure-blue.css",
              "src/styles.css"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "src/**/*.ts",
              "src/**/*.html"
            ]
          }
        }
      }
    }
  },
  "cli": {
    "analytics": false,
    "schematicCollections": [
      "angular-eslint"
    ],
    "cache": {
      "enabled": true
    }
  }
}
